{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "babel-plugin-direct-import",
      {
        "modules": [
          "@mui/material",
          "@mui/icons-material",
          "@emotion/react",
          "@emotion/styled"
        ]
      }
    ],
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties"
  ]
}